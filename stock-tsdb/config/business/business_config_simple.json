{
  "user_behavior": {
    "compression": "lz4",
    "name": "用户行为",
    "fields": {
      "1": {
        "description": "用户ID",
        "type": "string",
        "name": "user_id"
      },
      "2": {
        "description": "行为类型",
        "type": "string",
        "name": "action"
      },
      "3": {
        "description": "页面",
        "type": "string",
        "name": "page"
      },
      "4": {
        "description": "停留时长(秒)",
        "type": "int32",
        "name": "duration"
      }
    },
    "description": "用户点击、浏览等行为数据",
    "indexes": {
      "1": "timestamp",
      "2": "user_id",
      "3": "action"
    },
    "retention_days": 30,
    "block_size": 60,
    "cold_hot_config": {
      "enable_separation": true,
      "hot_data_days": 7,
      "cold_data_compression": "zstd",
      "hot_data_compression": "lz4",
      "disable_cold_compaction": true,
      "cleanup_interval_hours": 24
    }
  },
